INSERT INTO "دعاء المسافر" (duaa, id) VALUES ('الله أكبر ،الله أكبر ،الله أكبر ', NULL);
INSERT INTO "دعاء المسافر" (duaa, id) VALUES ('سبحان الذي سخرلنا هذا وما كنا له مقرنين وإنا إلى ربنا لمنقلبون', NULL);
INSERT INTO "دعاء المسافر" (duaa, id) VALUES ('اللهم إنا نسألُكَ في سفرنا هذا البرَّ والتقوى، ومن العمل ما ترضى', NULL);
INSERT INTO "دعاء المسافر" (duaa, id) VALUES ('اللهم هون علينا سفرنا هذا واطو عنا بعده، اللهم أنت الصاحب في السفر، والخليفة في الأهل، اللهم إني أعوذ بك من وعْثاءِ السفر، وكآبة المنظر وسوء المنقلب في المال والأهل', NULL);

-- Table: دعاء النظرة الاولى

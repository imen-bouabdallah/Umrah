-- Table: أدعية منوعة
CREATE TABLE IF NOT EXISTS "أدعية منوعة" (duaa TEXT, id NUMERIC PRIMARY KEY UNIQUE);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('ربنا آتنا في الدنيا حسنة وفي الآخرة حسنة وقنا عذاب النار', 1);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('سبحان الله وبحمده، سبحان الله العظيم، لا إله إلا الله وحده لا شريك له، له الملك، وله الحمد، وهو على كل شيء قدير', 2);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم اغفر لي، وارحمني، اللهم إني أسألك رضاك والجنة، وأعوذ بك من سخطك والنار، اللهم أصلح قلبي وعملي، اللهم اغفر لي ولوالدي', 3);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم يا مقلب القلوب ثبِّت قلوبنا على دينك', 4);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم إني أسألك موجبات رحمتك وعزائم مغفرتك والسلامة من كل إثم والفوز بالجنة والنجاة من النار', 5);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم إني أسألك الهدى والغنى والعفاف اللهم أعني على ذكرك وشكرك وحسن عبادتك', 6);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم إني أسألك من الخير كله ما علمت منه وما لم أعلم وأعوذ بك من الشر كله ما علمت منه وما لم أعلم', 7);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم إني أسألك الجنة وما قرب منها من قول وعمل وأعوذ بك من النار وما قرب منها من قول أو عمل', 8);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللهم إني أسألك النعيم المقيم الذي لا يحول ولا يزول أبداً', 9);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللَّهُمَّ رَبَّنَا لكَ الحَمْدُ أنْتَ قَيِّمُ السَّمَوَاتِ والأرْضِ، ولَكَ الحَمْدُ أنْتَ رَبُّ السَّمَوَاتِ والأرْضِ ومَن فِيهِنَّ، ولَكَ الحَمْدُ أنْتَ نُورُ السَّمَوَاتِ والأرْضِ ومَن فِيهِنَّ، أنْتَ الحَقُّ، وقَوْلُكَ الحَقُّ، ووَعْدُكَ الحَقُّ، ولِقَاؤُكَ الحَقُّ، والجَنَّةُ حَقٌّ، والنَّارُ حَقٌّ، والسَّاعَةُ حَقٌّ، اللَّهُمَّ لكَ أسْلَمْتُ، وبِكَ آمَنْتُ، وعَلَيْكَ تَوَكَّلْتُ، وإلَيْكَ خَاصَمْتُ، وبِكَ حَاكَمْتُ، فَاغْفِرْ لي ما قَدَّمْتُ وما أخَّرْتُ، وأَسْرَرْتُ وأَعْلَنْتُ، وما أنْتَ أعْلَمُ به مِنِّي، لا إلَهَ إلَّا أنْتَ', 10);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('اللَّهُمَّ اغْفِرْ لي خَطِيئَتي وَجَهْلِي، وإسْرَافِي في أَمْرِي، وَما أَنْتَ أَعْلَمُ به مِنِّي، اللَّهُمَّ اغْفِرْ لي جِدِّي وَهَزْلِي، وَخَطَئِي وَعَمْدِي، وَكُلُّ ذلكَ عِندِي، اللَّهُمَّ اغْفِرْ لي ما قَدَّمْتُ وَما أَخَّرْتُ، وَما أَسْرَرْتُ وَما أَعْلَنْتُ، وَما أَنْتَ أَعْلَمُ به مِنِّي، أَنْتَ المُقَدِّمُ وَأَنْتَ المُؤَخِّرُ، وَأَنْتَ علَى كُلِّ شيءٍ قَدِيرٌ', 11);
INSERT INTO "أدعية منوعة" (duaa, id) VALUES ('ربِّ اجعلْني لك شَكَّارًا لك ذَكَّارًا لك رهَّابًا لك مِطواعًا لك مُخبِتًا إليك أَوَّاهًا مُنيبًا ربِّ تقبَّلْ تَوْبتي واغسِلْ حَوبَتي وأَجِبْ دَعْوتي وثَبِّتْ حُجَّتي وسَدِّدْ لساني واهدِ قلبي واسلُلْ سَخيمةَ صدْري', 12);

-- Table: ادعية الطواف
